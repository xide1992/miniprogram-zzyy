<!--pages/dbs/dbs.wxml-->
<view class="weui-cells__title">表名</view>
<view class="weui-cells weui-cells_after-title">
	<view wx:for="{{tableNames}}" class="weui-cell weui-cell_example ">
		<view class="weui-cell__hd">
			<image src="{{icon}}" style="margin-right: 16px;vertical-align: middle;width:20px; height: 20px;"></image>
		</view>
		<view class="weui-cell__bd">{{item.name}}</view>
		<view class="weui-cell__ft">{{item.comment}}</view>
	</view>
</view>